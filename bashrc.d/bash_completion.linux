if [ "$(uname)" == "Darwin" ]; then
    true;
elif [ "$(expr substr $(uname -s) 1 5)" == "Linux" ]; then
  if [ -f /etc/bash_completion ]; then
    . /etc/bash_completion
  fi
  if [ -f $HOME/projects/hdfs-completer/hdfs_completer_unix.sh ]; then
    . $HOME/projects/hdfs-completer/hdfs_completer_unix.sh
  fi
fi
